{
  "extends": 
    [
      "tslint:latest",
      "tslint-eslint-rules",
      "tslint-config-prettier"
    ],
    "rules": {
      "no-console":false,
      "max-classes-per-file": false,
      "variable-name":false,
      "no-implicit-dependencies": false,
      "only-arrow-functions": false,
      "ordered-imports": false
      
    }
}
